{
  "info": {
    "name": "OMIND Prototype",
    "_postman_id": "omind-proto",
    "description": "Basic calls for upload and retrieval",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "item": [
    {
      "name": "Upload Call",
      "request": {
        "method": "POST",
        "header": [],
        "body": {
          "mode": "formdata",
          "formdata": [
            { "key": "file", "type": "file", "src": "" }
          ]
        },
        "url": { "raw": "http://localhost:5000/api/upload", "protocol": "http", "host": ["localhost"], "port": "5000", "path": ["api","upload"] }
      }
    },
    {
      "name": "Get Call By Id",
      "request": {
        "method": "GET",
        "header": [],
        "url": { "raw": "http://localhost:5000/api/calls/:id", "protocol": "http", "host": ["localhost"], "port": "5000", "path": ["api","calls", ":id"] }
      }
    },
    {
      "name": "Auth - Mock Login",
      "request": {
        "method": "POST",
        "header": [ { "key": "Content-Type", "value": "application/json" } ],
        "body": { "mode": "raw", "raw": "{ \"username\": \"demo\" }" },
        "url": { "raw": "http://localhost:5000/api/auth/login", "protocol": "http", "host": ["localhost"], "port": "5000", "path": ["api","auth","login"] }
      }
    },
    {
      "name": "List Calls",
      "request": { "method": "GET", "url": { "raw": "http://localhost:5000/api/calls", "protocol": "http", "host": ["localhost"], "port": "5000", "path": ["api","calls"] } }
    }
  ]
}
